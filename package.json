{
	"name": "maexal.dev",
	"version": "1.1.0",
	"private": true,
	"description": "Business website and portfolio of Maexal",
	"keywords": [
		"maexal"
	],
	"homepage": "https://maexal.dev/",
	"bugs": {
		"url": "https://github.com/maexal/maexal.dev/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/maexal/maexal.dev.git"
	},
	"license": "MIT",
	"author": "Max Altena <max@maexal.dev> (https://maexal.dev/)",
	"scripts": {
		"analyze": "cross-env ANALYZE=true npm run build:ssr",
		"analyze:browser": "cross-env BUNDLE_ANALYZE=browser npm run build:ssr",
		"analyze:server": "cross-env BUNDLE_ANALYZE=server npm run build:ssr",
		"build": "npm run build:ssr",
		"build:ssg": "cross-env NODE_ENV=production next build && next export -o build",
		"build:ssr": "cross-env NODE_ENV=production next build",
		"dev": "cross-env NODE_ENV=development next",
		"eslint": "eslint .",
		"eslint:fix": "eslint . --fix",
		"prettier": "prettier **/*",
		"prettier:fix": "prettier **/* --write",
		"sort-package-json": "sort-package-json",
		"start": "cross-env NODE_ENV=production next start",
		"prepare": "husky install"
	},
	"dependencies": {
		"framer-motion": "^5.6.0",
		"next": "^12.0.7",
		"next-build-id": "^3.0.0",
		"next-localization": "^0.11.0",
		"next-pwa": "^5.4.4",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.6",
		"react-toastify": "^8.1.0",
		"redux": "^4.1.2",
		"styled-components": "^5.3.3",
		"sweetalert2": "^11.3.4"
	},
	"devDependencies": {
		"@next/bundle-analyzer": "^12.0.8",
		"@types/node": "^17.0.8",
		"@types/react": "^17.0.38",
		"@types/react-dom": "^17.0.11",
		"@types/react-redux": "^7.1.21",
		"@types/styled-components": "^5.1.19",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"autoprefixer": "^10.4.2",
		"babel-plugin-styled-components": "^2.0.2",
		"cross-env": "^7.0.3",
		"eslint": "^7.25.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.1.7",
		"postcss": "^8.4.5",
		"prettier": "^2.5.1",
		"redux-devtools-extension": "^2.13.9",
		"sass": "^1.47.0",
		"sort-package-json": "^1.53.1",
		"tailwindcss": "^3.0.13",
		"typescript": "^4.5.4",
		"webpack": "^5.66.0"
	},
	"engines": {
		"node": ">= 14",
		"npm": ">= 6.12.1",
		"yarn": ">= 1.19.1"
	}
}
