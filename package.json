{
	"name": "maexal.dev",
	"version": "1.0.0",
	"private": true,
	"description": "Business website and portfolio of Maexal",
	"keywords": [
		"maexal"
	],
	"homepage": "https://maexal.dev/",
	"bugs": {
		"url": "https://github.com/maexal/maexal.dev/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/maexal/maexal.dev.git"
	},
	"license": "MIT",
	"author": "Max Altena <max@maexal.dev> (https://maexal.dev/)",
	"scripts": {
		"build": "next build",
		"dev": "next dev",
		"eslint": "eslint .",
		"eslint:fix": "eslint . --fix",
		"prettier": "prettier **/*",
		"prettier:fix": "prettier **/* --write",
		"preview": "netlify build && netlify deploy",
		"sort-package-json": "sort-package-json",
		"start": "next start",
		"test": "snyk test && echo \"Error: test not specified\" && exit 0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": "npm run prettier:fix",
		"*.{js,jsx,ts,tsx}": "npm run eslint:fix",
		"package.json": "npm run sort-package-json"
	},
	"browserslist": [
		"last 1 version",
		"> 0.25%",
		"maintained node versions",
		"not dead"
	],
	"dependencies": {
		"@netlify/plugin-nextjs": "^1.0.3",
		"@netlify/plugin-sitemap": "^0.6.1",
		"framer-motion": "^3.1.1",
		"netlify-plugin-cache-nextjs": "^1.5.2",
		"netlify-plugin-image-optim": "^0.4.0",
		"netlify-plugin-minify-html": "^0.3.0",
		"netlify-plugin-submit-sitemap": "^0.2.3",
		"next": "^10.0.4",
		"next-localization": "^0.10.0",
		"next-pwa": "^5.0.1",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-particles-js": "^3.4.1",
		"sass": "^1.30.0"
	},
	"devDependencies": {
		"@types/node": "^14.14.16",
		"@types/react": "^17.0.0",
		"@typescript-eslint/eslint-plugin": "^4.11.1",
		"@typescript-eslint/parser": "^4.11.1",
		"autoprefixer": "^10.1.0",
		"eslint": "^7.16.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-prettier": "^3.3.0",
		"eslint-plugin-react": "^7.21.5",
		"globby": "^11.0.1",
		"husky": "^4.3.6",
		"lint-staged": "^10.5.3",
		"postcss": "^8.2.1",
		"prettier": "^2.2.1",
		"snyk": "^1.437.3",
		"sort-package-json": "^1.48.0",
		"tailwindcss": "^2.0.2",
		"typescript": "^4.1.3"
	},
	"engines": {
		"node": ">= 8.17.0",
		"npm": ">= 6.12.1",
		"yarn": ">= 1.19.1"
	}
}
