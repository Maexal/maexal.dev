{
	"name": "maexal.dev",
	"version": "1.0.0",
	"private": true,
	"description": "Business website and portfolio of Maexal",
	"keywords": [
		"maexal"
	],
	"homepage": "https://maexal.dev/",
	"bugs": {
		"url": "https://github.com/maexal/maexal.dev/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/maexal/maexal.dev.git"
	},
	"license": "MIT",
	"author": "Max Altena <max@maexal.dev> (https://maexal.dev/)",
	"scripts": {
		"analyze": "cross-env ANALYZE=true next build",
		"analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
		"analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
		"build": "next build",
		"dev": "next",
		"eslint": "eslint .",
		"eslint:fix": "eslint . --fix",
		"prettier": "prettier **/*",
		"prettier:fix": "prettier **/* --write",
		"preview": "netlify build && netlify deploy",
		"sort-package-json": "sort-package-json",
		"start": "next start",
		"test": "snyk test && echo \"Error: test not specified\" && exit 0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": "npm run prettier:fix",
		"*.{js,jsx,ts,tsx}": "npm run eslint:fix",
		"package.json": "npm run sort-package-json"
	},
	"browserslist": [
		"last 1 version",
		"> 0.25%",
		"maintained node versions",
		"not dead"
	],
	"dependencies": {
		"emailjs-com": "^2.6.4",
		"framer-motion": "^3.2.1",
		"next": "^10.0.5",
		"next-localization": "^0.11.0",
		"next-pwa": "^5.0.3",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-particles-js": "^3.4.1",
		"react-redux": "^7.2.2",
		"react-toastify": "^6.2.0",
		"redux": "^4.0.5",
		"redux-devtools-extension": "^2.13.8",
		"sweetalert2": "^10.13.0"
	},
	"devDependencies": {
		"@netlify/plugin-lighthouse": "^1.4.2",
		"@netlify/plugin-nextjs": "^1.0.5",
		"@netlify/plugin-sitemap": "^0.6.2",
		"@next/bundle-analyzer": "^10.0.5",
		"@types/node": "^14.14.20",
		"@types/react": "^17.0.0",
		"@types/react-redux": "^7.1.15",
		"@typescript-eslint/eslint-plugin": "^4.13.0",
		"@typescript-eslint/parser": "^4.13.0",
		"autoprefixer": "^10.2.1",
		"cross-env": "^7.0.3",
		"eslint": "^7.17.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.22.0",
		"husky": "^4.3.7",
		"lint-staged": "^10.5.3",
		"netlify-plugin-cache-nextjs": "^1.5.2",
		"netlify-plugin-html-validate": "^0.2.0",
		"netlify-plugin-image-optim": "^0.4.0",
		"netlify-plugin-minify-html": "^0.3.0",
		"netlify-plugin-snyk": "^1.2.1",
		"netlify-plugin-subfont": "^5.0.6",
		"netlify-plugin-submit-sitemap": "^0.2.3",
		"postcss": "^8.2.4",
		"prettier": "^2.2.1",
		"sass": "^1.32.4",
		"snyk": "^1.437.4",
		"sort-package-json": "^1.48.1",
		"tailwindcss": "^2.0.2",
		"typescript": "^4.1.3"
	},
	"engines": {
		"node": ">= 8.17.0",
		"npm": ">= 6.12.1",
		"yarn": ">= 1.19.1"
	}
}
