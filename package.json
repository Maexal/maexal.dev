{
	"name": "maexal.dev",
	"version": "1.0.0",
	"private": true,
	"description": "Business website and portfolio of Maexal",
	"keywords": [
		"maexal"
	],
	"homepage": "https://maexal.dev/",
	"bugs": {
		"url": "https://github.com/maexal/maexal.dev/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/maexal/maexal.dev.git"
	},
	"license": "MIT",
	"author": "Max Altena <max@maexal.dev> (https://maexal.dev/)",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"postbuild": "next-on-netlify",
		"start": "next start",
		"eslint": "eslint .",
		"eslint:fix": "eslint . --fix",
		"prettier": "prettier **/*",
		"prettier:fix": "prettier **/* --write",
		"test": "snyk test && echo \"Error: test not specified\" && exit 0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": "npm run prettier:fix",
		"*.js": "npm run eslint:fix"
	},
	"browserslist": [
		"last 1 version",
		"> 0.25%",
		"maintained node versions",
		"not dead"
	],
	"dependencies": {
		"next": "^10.0.3",
		"next-on-netlify": "^2.6.3",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"sass": "^1.29.0"
	},
	"devDependencies": {
		"@types/node": "^14.14.10",
		"@types/react": "^17.0.0",
		"autoprefixer": "^10.0.4",
		"eslint": "^7.14.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"globby": "^11.0.1",
		"husky": "^4.3.0",
		"lint-staged": "^10.5.2",
		"postcss": "^8.1.10",
		"prettier": "^2.2.1",
		"snyk": "^1.433.0",
		"tailwindcss": "^2.0.1",
		"typescript": "^4.1.2"
	},
	"engines": {
		"node": ">= 8.17.0",
		"npm": ">= 6.12.1",
		"yarn": ">= 1.19.1"
	}
}
